<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nb.springboot.nboot.mapper.StudentsMapper">

    <insert id="insertMany" parameterType="com.nb.springboot.nboot.pojo.Student">
        insert into student (id,name,t_id) values
        <foreach collection="list" index="index" item="item" separator=",">
            (#{item.id},#{item.name},#{item.tId})
        </foreach>
    </insert>

    <select id="selectIf" parameterType="map" resultType="com.nb.springboot.nboot.pojo.Student">
        select * from student where 1=1
        <if test="name!=null">
            and name=#{name}
        </if>
        <if test="tId!=null">
            and t_id=#{tId}
        </if>
    </select>

</mapper>